---
export interface Props {

}

const {
	...input
} = Astro.props as Props

var array = Object.values(input)

const slots = Object.keys(Astro.slots)

console.log(input)
---

{ array.map((item, i) => {
	if (''+i in slots) return <Fragment set:html={Astro.slots.render(''+i, [array.at(i)])}/>
	if (i === array.length -1 && Astro.slots.has('last')) return <Fragment set:html={Astro.slots.render('last', [array.pop()])}/>
	return <Fragment set:html={Astro.slots.render('default', [item])}/>
})}
